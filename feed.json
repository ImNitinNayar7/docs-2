---
layout:
---

[
  {% assign landing = site.docs_landing | where: 'draft', 'false' %}{% for landing in site.docs_landing %}{% if article.draft %}{% else %}
  {
    {% if article.title == "" %}{% else %}"title" : {{ article.title | jsonify }},
    {% endif %}"docs_landing" : "true",
    "url"     : "/docs/{{ article.title | slugify }}",{% if article.title %}
    "slug"     : {{ article.title | slugify | jsonify }},{% endif %}
    "content": {{ article.content | jsonify }}
  },
  {% endif %}{% endfor %}{% assign quick-start = site.docs_quick-start | where: 'draft', 'false' %}{% for article in quick-start %}{% if article.draft %}{% else %}
  {
    {% if article.title == "" %}{% else %}"title" : {{ article.title | jsonify }},
    {% endif %}"docs_quick-start" : "true",
    "url"     : "/docs/quick-start/{{ article.title | slugify }}",{% if article.title %}
    "slug"     : {{ article.title | slugify | jsonify }},{% endif %}
    "content": {{ article.content | jsonify }}
  },
  {% endif %}{% endfor %}{% assign using-snyk = site.docs_using-snyk | where: 'draft', 'false' %}{% for article in using-snyk %}{% if article.draft %}{% else %}
  {
    {% if article.title == "" %}{% else %}"title" : {{ article.title | jsonify }},
    {% endif %}"docs_using-snyk" : "true",
    "url"     : "/docs/using-snyk/{{ article.title | slugify }}",{% if article.title %}
    "slug"     : {{ article.title | slugify | jsonify }},{% endif %}
    "content": {{ article.content | jsonify }}
  },
  {% endif %}{% endfor %}{% assign faqs = site.docs_faqs | where: 'draft', 'false' %}{% for article in faqs %}{% if article.draft %}{% else %}
  {
    {% if article.title == "" %}{% else %}"title" : {{ article.title | jsonify }},
    {% endif %}"docs_faqs" : "true",
    "url"     : "/docs/faqs/{{ article.title | slugify }}",{% if article.title %}
    "slug"     : {{ article.title | slugify | jsonify }},{% endif %}
    "content": {{ article.content | jsonify }}
  }{% endif %}{% if forloop.last %}{% else %},{% endif %}{% endfor %}
]
